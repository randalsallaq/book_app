  
  
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles/base.css">
    <title>Book-API</title>
</head>
<body>
    <%book.forEach(element  => { %>
        <div data-id>
        <h2>Title :<%= element.title %></h2>
        <h3>Author : <%= element.author %></h3>
        <img src="<%= element.image_url %>" alt="">
        <p>Description: <%= element.descriptions %></p>
        <p>ISBN: <%= element.isbn %></p>
    </div>
    <input id="detailsBtn" type="submit" value="Show Details" onclick="location.href = `/book/${<%= element.id %>}`;" ><br>

    <% }); %>

    <p>Total Books: <%= total %></p>
</body>
</html>